{
  "name": "cli-app",
  "version": "0.0.0",
  "private": true,
  "description": "Small description for cli-app goes here",
  "repository": "",
  "license": "MIT",
  "author": "",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "vite build",
    "format": "prettier . --cache --write",
    "lint": "concurrently \"pnpm:lint:*(!fix)\" --names \"lint:\" --prefixColors auto",
    "lint:fix": "concurrently \"pnpm:lint:*:fix\" --names \"fix:\" --prefixColors auto && pnpm format",
    "lint:format": "prettier . --cache --check",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "lint:types": "glint",
    "start": "vite",
    "test": "vite build --mode development && testem ci"
  },
  "exports": {
    "./tests/*": "./tests/*",
    "./*": "./app/*"
  },
  "devDependencies": {
    "@babel/core": "^7.27.1",
    "@babel/plugin-transform-runtime": "^7.27.1",
    "@babel/plugin-transform-typescript": "^7.27.1",
    "@babel/runtime": "^7.27.1",
    "@ember/app-tsconfig": "^1.0.3",
    "@ember/optional-features": "^2.2.0",
    "@ember/test-helpers": "^5.2.2",
    "@ember/test-waiters": "^4.1.0",
    "@embroider/compat": "^4.1.0",
    "@embroider/config-meta-loader": "^1.0.0",
    "@embroider/core": "^4.1.0",
    "@embroider/macros": "^1.18.0",
    "@embroider/router": "^3.0.1",
    "@embroider/vite": "^1.1.5",
    "@eslint/js": "^9.27.0",
    "@glimmer/component": "^2.0.0",
    "@glint/core": "alpha",
    "@glint/environment-ember-loose": "alpha",
    "@glint/environment-ember-template-imports": "alpha",
    "@glint/template": "alpha",
    "@glint/tsserver-plugin": "alpha",
    "@nullvoxpopuli/eslint-configs": "^5.3.4",
    "@rollup/plugin-babel": "^6.0.4",
    "@types/qunit": "^2.19.12",
    "@types/rsvp": "^4.0.9",
    "babel-plugin-ember-template-compilation": "^2.4.1",
    "concurrently": "^9.1.2",
    "decorator-transforms": "^2.3.0",
    "ember-modifier": "^4.2.2",
    "ember-page-title": "^9.0.2",
    "ember-qunit": "^9.0.3",
    "ember-resolver": "^13.1.1",
    "ember-source": "6.8.0-alpha.6",
    "ember-template-lint": "^7.7.0",
    "eslint": "^9.27.0",
    "globals": "^16.1.0",
    "prettier": "^3.5.3",
    "prettier-plugin-ember-template-tag": "^2.0.5",
    "qunit": "^2.24.1",
    "qunit-dom": "^3.4.0",
    "testem": "^3.16.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.32.1",
    "vite": "^7.1.3"
  },
  "engines": {
    "node": ">= 22"
  },
  "pnpm": {
    "patchedDependencies": {
      "ember-source": "patches/ember-source.patch"
    }
  },
  "ember": {
    "edition": "octane"
  }
}
