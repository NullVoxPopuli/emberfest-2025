{
  "extends": "@ember/app-tsconfig",
  "glint": {
    "environment": [
      "ember-loose",
      "ember-template-imports"
    ]
  },
  "compilerOptions": {
    "allowJs": true,
    "lib": [
      "es2024",
      "esnext",
      "dom",
      "dom.iterable"
    ],
    "paths": {
      "node-app/tests/*": [
        "./tests/*"
      ],
      "node-app/*": [
        "./app/*"
      ],
      "*": [
        "./types/*"
      ]
    },
    "types": [
      "ember-source/types",
      "@embroider/core/virtual",
      "vite/client"
    ]
  },
}
